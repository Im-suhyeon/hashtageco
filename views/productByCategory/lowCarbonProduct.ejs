    <style>
      .bd-placeholder-img {
        font-size: 1.125rem;
        text-anchor: middle;
        -webkit-user-select: none;
        -moz-user-select: none;
        user-select: none;
      }

      @media (min-width: 768px) {
        .bd-placeholder-img-lg {
          font-size: 3.5rem;
        }
      }

      .b-example-divider {
        height: 3rem;
        background-color: rgba(0, 0, 0, 0.1);
        border: solid rgba(0, 0, 0, 0.15);
        border-width: 1px 0;
        box-shadow: inset 0 0.5em 1.5em rgba(0, 0, 0, 0.1), inset 0 0.125em 0.5em rgba(0, 0, 0, 0.15);
      }

      .b-example-vr {
        flex-shrink: 0;
        width: 1.5rem;
        height: 100vh;
      }

      .bi {
        vertical-align: -0.125em;
        fill: currentColor;
      }

      .nav-scroller {
        position: relative;
        z-index: 2;
        height: 2.75rem;
        overflow-y: hidden;
      }

      .nav-scroller .nav {
        display: flex;
        flex-wrap: nowrap;
        padding-bottom: 1rem;
        margin-top: -1px;
        overflow-x: auto;
        text-align: center;
        white-space: nowrap;
        -webkit-overflow-scrolling: touch;
      }
    </style>
  </head>
  <body>
    <main>
      <section class="py-5 text-center container">
        <div class="row py-lg-5">
          <div class="col-lg-6 col-md-8 mx-auto">
            <h1 class="fw-light">LowCarbon Product</h1>
            <p class="lead text-muted">저탄소 제품 둘러보기</p>
            <p>
              <!-- 해시태그 버튼 -->
              <!-- 버튼 라우트 설정 아직 안됨 -->
              <a href="/zeroWasteProduct/1" class="btn btn-primary my-2">제로웨이스트</a>
              <a href="/upcyclingProduct/1" class="btn btn-primary my-2">업사이클링</a>
              <a href="/lowCarbonProduct/1" class="btn btn-primary my-2">저탄소</a>
            </p>
          </div>
        </div>
      </section>
      <div class="album py-5 bg-light">
        <div class="container">
          <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3">
            <% list.forEach(s => { %>
            <!-- 상품 나열 전체 -->

            <!-- 상품 나열 개별 -->
            <!-- 일단 페이지에 상품 15개 띄우는 거로 했지만 상품 1~nnn 까지 반복문 써서 띄워야 할 듯 -->
            <!-- 상품 1 -->
            <div class="col">
              <div class="card shadow-sm">
                <svg
                  class="bd-placeholder-img card-img-top"
                  width="100%"
                  height="0"
                  xmlns="http://www.w3.org/2000/svg"
                  role="img"
                  aria-label="Placeholder: Thumbnail"
                  preserveAspectRatio="xMidYMid slice"
                  focusable="false"
                >
                  <img src="<%=s.dataValues.image_link%>" />
                  <title>Placeholder</title>
                  <rect width="100%" height="100%" fill="#55595c" />
                </svg>
                <!-- <img src="<%=s.dataValues.image_link%>"/> -->
                <div class="card-body">
                  <p class="card-text" style="font-weight: bold"><%= s.dataValues.product_name %></p>
                  <p class="card-text"><%= s.dataValues.price %>원</p>
                  <div class="d-flex justify-content-between align-items-center">
                    <div class="btn-group">
                      <!-- 버튼 라우트 설정 아직 안됨 -->
                      <a href="/productdetail/lowcarbon/<%= parseInt(s.dataValues.product_id) %>" class="btn btn-sm btn-outline-secondary">Detail</a>
                      <button type="button" class="btn btn-sm btn-outline-secondary" selected="true">Scrap</button>
                    </div>
                    <!-- 스크랩수 정렬 구현 못하면 지우기 -->
                    <small class="text-muted">스크랩수 넣기</small>
                  </div>
                </div>
              </div>
            </div>

            <% }) %>
          </div>
        </div>
      </div>
      <div class="py-5 text-center container">
        <nav class="d-flex justify-content-center" aria-label="Page navigation example">
          <ul class="pagination">
            <li class="page-item">
              <a class="page-link" href="#" aria-label="Previous">
                <span aria-hidden="true">&laquo;</span>
              </a>
            </li>
            <li class="page-item"><a class="page-link" href="/lowCarbonProduct/1">1</a></li>
            <li class="page-item"><a class="page-link" href="/lowCarbonProduct/2">2</a></li>
            <li class="page-item"><a class="page-link" href="/lowCarbonProduct/3">3</a></li>
            <li class="page-item"><a class="page-link" href="/lowCarbonProduct/4">4</a></li>
            <li class="page-item"><a class="page-link" href="/lowCarbonProduct/5">5</a></li>
            <li class="page-item"><a class="page-link" href="/lowCarbonProduct/6">6</a></li>
            <li class="page-item"><a class="page-link" href="/lowCarbonProduct/7">7</a></li>
            <li class="page-item"><a class="page-link" href="/lowCarbonProduct/8">8</a></li>
            <li class="page-item"><a class="page-link" href="/lowCarbonProduct/9">9</a></li>
            <li class="page-item"><a class="page-link" href="/lowCarbonProduct/10">10</a></li>
            <li class="page-item">
              <a class="page-link" href="#" aria-label="Next">
                <span aria-hidden="true">&raquo;</span>
              </a>
            </li>
          </ul>
        </nav>
      </div>
    </main>

    <script
      src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js"
