  <body>
    <div class="container">
      <main>
        <div class="py-5 text-center">
          <!-- h2태그 내부에 적힌 거 변경해도 됨 -->
          <h2 style="font-weight: bold">Profile</h2>
          <!-- p태그 내부에 적힌 거 변경해도 됨 -->
          <p class="lead">프로필</p>
        </div>

        <div id="profile-update-form" action="/profile" method = "post" class="row g-5 d-flex justify-content-center justify-content-center">
          <!-- 프로필 기본정보 -->
          <!-- 저장된 user의 기본정보들이 불러와져 있어야함-->
          <!-- 이름, 닉네임, 이메일은 변경 불가능 / 비밀번호만 변경 가능 -->
          <div class="col-md-7 col-lg-8">
            <!-- <form class="needs-validation" novalidate id="form"> -->
              <div class="row g-3">
                <!-- 기존 이름 -->
                <div class="col-sm-6">
                  <label for="firstName" class="form-label">Name</label>
                  <!-- <input name="name" id="name" type="text" class="form-control" placeholder="기존 이름" value="" required /> -->
                  <p class="form-control"><%= memberData[0].dataValues.name %></p>
                  <div id="nameError" class="invalid-feedback">이름을 다시 입력해주세요</div>
                </div>
                <!-- 기존 닉네임 -->
                <div class="col-sm-6">
                  <label for="lastName" class="form-label">Nickname</label>
                  <!-- <input name="nickname" id="nickname" type="text" class="form-control" placeholder="기존 닉네임" value="" required /> -->
                  <p class="form-control"><%= memberData[0].dataValues.nickname %></p>

                  <div id="nicknameError" class="invalid-feedback">닉네임을 다시 입력해주세요</div>
                </div>
                <!-- 기존 이메일 -->
                <div class="col-12">
                  <label for="email" class="form-label">Email</label>
                  <!-- <input name="email" id="email" type="email" class="form-control" placeholder="기존 이메일" /> -->
                  <p class="form-control"><%= memberData[0].dataValues.email %></p>

                  <div id="emailError" class="invalid-feedback">이메일을 다시 입력해주세요</div>
                </div>
                <!-- 기존 비밀번호 -->
                <div class="col-12">
                  <label for="address" class="form-label">Password</label>
                  <!-- <input name="password" id="password" type="password" class="form-control" placeholder="기존 비밀번호" required /> -->
                  <p class="form-control"><%= memberData[0].dataValues.password %></p>

                  <div id="passwordError" class="invalid-feedback">비밀번호를 다시 입력해주세요</div>
                </div>
                <!-- 새로운 비밀번호 -->
                <form action="profile" method="POST">
                  <div class="col-12">
                    <label for="address" class="form-label">New Password</label>
                    <input name="newpassword" id="newpassword" type="password" class="form-control" placeholder="새로운 비밀번호 (8자리 이상 16자리 이하)" required />
                    <div id="passwordError" class="invalid-feedback">비밀번호를 다시 입력해주세요</div>
                  </div>
                   <!-- 위아래 간격 조정 -->
                  <br />
                  <br />
                  <!-- 프로필수정 버튼 -->
                  <button style="background-color: rgb(49, 110, 49); color: white" class="w-100 btn btn-lg" type="submit">비밀번호 변경하기</button>

                <!-- </form> -->
              </div>

            
              <!-- 위아래 간격 조정 -->
              <br />
              <br />
            </form>
          </div>
        </div>
      </main>
    </div>

